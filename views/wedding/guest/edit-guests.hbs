<a href='/wedding/details/{{_id}}'>Main weeding page</a>
<h2>Edit Guests</h2>
{{#if noGuest}}
<form action="/wedding/edit-guests/{{_id}}" method="post">
{{#each guests}}
    <label for={{this}}>Edit guest e-mail</label>
    <input name={{this}} value={{this}}>
    </br>
    
{{/each}}
<button type="submit">Edit guests</button>
</form>
{{else}}
<p>No guests to edit</p>
{{/if}}